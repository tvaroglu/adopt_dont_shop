<div style="padding-left: 20px;">
  <h1>Edit Shelter</h1>
  <%= form_with url: "/admin/shelters/#{@shelter.id}", method: :patch, local: true do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :city %>
    <%= f.text_field :city %>

    <%= f.label :foster_program %>
    <%= f.check_box :foster_program %>

    <%= f.label :rank %>
    <%= f.number_field :rank %>

    <%= f.submit %>
  <% end %>
</div>
